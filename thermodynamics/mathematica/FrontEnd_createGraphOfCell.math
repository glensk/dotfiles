createGraphOfCell[cell_, hue_Real: 1.0] := Module[
  {
    G = {0, 0, 0}, c1 = cell[[1]], c2 = cell[[2]], c3 = cell[[3]]
  },
  line1 = Line[{G, c1, c1 + c2, c2, G, c3, c3 + c2, c1 + c2 + c3, c1 + c3, c3}];
  line2 = Line[{c1, c1 + c3}];
  line3 = Line[{c2, c2 + c3}];
  line4 = Line[{c1 + c2, c1 + c2 + c3}];

  Graphics3D[{Hue[hue], line1, line2, line3, line4}]
];
